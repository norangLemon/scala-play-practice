
# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           presentation.controllers.HomeController.index

GET     /cookie                     presentation.controllers.CookieController.getCookieList

GET     /cookie/:id                 presentation.controllers.CookieController.getCookie(id: String)

POST    /cookie/:id                 presentation.controllers.CookieController.createCookie(id: String)

#PUT     /cookie/:id                 presentation.controllers.CookieController.updateCookie(id: String)
# PUT     /cookie/inventory/:id       controllers.CookieController.updateInventory(id: String)
# PUT     /cookie/wallet/:id          controllers.CookieController.updateWallet(id: String)

#GET     /assets/*file               presentation.controllers.Assets.versioned(path="/public", file: Asset)

